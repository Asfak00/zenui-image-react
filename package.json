    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.0",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.0",

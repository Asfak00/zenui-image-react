    "placeholder",
    "skeleton"
  ],
  "author": "Asfak Ahmed",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asfak00/zenui-image-react.git"
  },
  "bugs": {
    "url": "https://github.com/Asfak00/zenui-image-react/issues"
  },
  "scripts": {
